<script type="text/javascript">
function Apiformvalid() {
    var email = document.getElementById("emailID").value;
    var mailformat = /^[a-z0-9]([a-z0-9_\-\.\+]*)@([a-z0-9_\-\.]+)(\.[a-z]{2,4}){1,2}$/i;
    var nexturl = "https://www.manageengine.com/products/service-desk/incident-management-kit.zip";

    if (!mailformat.test(email)) {
        $('#emailID').focus().addClass('error');
        $('#email_err_msg').text('Entrez votre adresse e-mail.').show();
        return false;
    }
    else {
        getMarkvalRegional();
        try {
            $zoho.salesiq.visitor.customaction('{\'eventCategory\':\'MSP Guide\',\'eventAction\':\'ServiceDesk MSP\',\'eventLabel\':\'MSP guide chapter 10 - French\',\'customID\':\'-\'}');
        } catch (exp) {}

        if (confirm) {
            var nexturl = "https://www.manageengine.com/fr/service-desk-msp/msp-guide-thanks.html";
            $("#nexturl").val(nexturl);
            window.open("https://www.manageengine.com/products/service-desk-msp/msp-software-evaluation-kit.zip");
        } else {
            return false;
        }
        $("#nexturl").val(nexturl);
        $('#email_err_msg').hide();
        document.Apiform.submit();
    }
    $('.error').first().focus();
    return false;
}
</script> 


