{
  "extends": [
    "development"
  ],
  "hints": {
    "css-prefix-order": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "@property",
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 124",
    "not firefox <= 124",
    "not ios_saf <= 17.4",
    "not safari <= 17.4"
  ]
}